<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Larry Shields</Authors>
    <Description>Entity Framework Library for interacting with the Nba Stats Database</Description>
    <PackageId>LSWebWorld.NbaStats.Data</PackageId>
    <Product>LSWebWorld.NbaStats.Data</Product>
    <Version>1.0.4</Version>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <RepositoryUrl>https://github.com/runstache/netcore-nbastats-data</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.InMemory" Version="5.0.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.2" />
  </ItemGroup>

</Project>
